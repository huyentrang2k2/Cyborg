	</head>

	<body>
		<!-- ***** Preloader Start ***** -->
		<div id="js-preloader" class="js-preloader">
			<div class="preloader-inner">
				<span class="dot"></span>
				<div class="dots">
					<span></span>
					<span></span>
					<span></span>
				</div>
			</div>
		</div>
		<!-- ***** Preloader End ***** -->

		<!-- ***** Header Area Start ***** -->
		<header class="header-area header-sticky">
			<div class="container">
				<div class="row">
					<div class="col-12">
						<nav class="main-nav">
							<!-- ***** Logo Start ***** -->
							<a href="/" class="logo">
								<img src="assets/images/logo.png" alt="" />
							</a>
							<!-- ***** Logo End ***** -->
							<!-- ***** Search End ***** -->
							<div class="search-input">
								<form id="search" action="#">
									<input
										type="text"
										placeholder="Search"
										id="searchText"
										name="searchKeyword"
										onkeypress="handle"
									/>
									<i class="fa fa-search"></i>
								</form>
							</div>
							<!-- ***** Search End ***** -->
							<!-- ***** Menu Start ***** -->
							<ul class="nav me-2">
								<li><a href="/" class="active">Home</a></li>
								<li><a href="/album">Album</a></li>
								<li><a href="/lightstick">Lightstick</a></li>
								<li><a href="/cart">Cart</a></li>
							</ul>
							<li class="d-flex">
								<a href="/signin" class="me-1"
									><img src="/images/picwish.png" alt="" style="width: 45px" />
								</a>
								<a href="#" onclick="logOut()"
									><img
										src="/images/png-transparent-logging-out-log-off-logout-thumbnail (1).png"
										style="width: 35px; margin-top: 5px"
										alt=""
								/></a>
							</li>

							<a class="menu-trigger">
								<span>Menu</span>
							</a>
							<!-- ***** Menu End ***** -->
						</nav>
					</div>
				</div>
			</div>
		</header>
	</body>
	<script>
		const logOut = () => {
			axios
				.post('http://localhost:3000/api/logout')
				.then((res) => {
					console.log(res);
				})
				.catch((err) => console.log(err));
			window.location.href = 'http://localhost:3000/signin';
		};
	</script>
